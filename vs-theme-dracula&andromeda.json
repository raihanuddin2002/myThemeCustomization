                "settings": {
                    "foreground": "#ffcc7b",
                    "fontStyle": "italic",
                }
            },
            {
                "scope":[
                    "constant.numeric.css"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
             /* ================================
                * root
             *=================================*/
            {
